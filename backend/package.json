{
  "name": "mia-experiment-server",
  "version": "1.0.0",
  "description": "MIA Experiment Server",
  "main": "src/app.ts",
  "scripts": {
    "start": "cross-env NODE_ENV=development nodemon",
    "start-prod": "cross-env NODE_ENV=production node build/app.js",
    "force-sync-then-start-prod": "cross-env NODE_ENV=production FORCE_SYNC=true node build/app.js",
    "build": "tsc",
    "inspect": "nodemon --inspect src/app.ts",
    "migrate": "npx sequelize-cli db:migrate",
    "seed": "npx sequelize-cli db:seed:all",
    "seed-prod": "npx sequelize-cli db:seed:all --env production",
    "force-sync-dev": "cross-env NODE_ENV=development ts-node src/migrate.ts",
    "force-sync-prod": "cross-env NODE_ENV=production ts-node src/migrate.ts --env production",
    "setup-dev": "npm run force-sync-dev && npm run seed",
    "test": "jest",
    "lint": "npm run lint:eslint ",
    "lint:eslint": "eslint --ignore-path .gitignore --ext .ts src/",
    "lint:fix": "npm run lint:eslint -- --fix"
  },
  "author": "Shan Rauf",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "celebrate": "^11.0.0",
    "connect-history-api-fallback": "^1.6.0",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "cross-env": "^6.0.3",
    "dotenv": "^8.2.0",
    "errorhandler": "^1.5.1",
    "event-dispatch": "^0.4.1",
    "eventemitter3": "^4.0.0",
    "express": "^4.17.1",
    "express-basic-auth": "^1.2.0",
    "express-jwt": "^5.3.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "mailgun-js": "^0.22.0",
    "method-override": "^3.0.0",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.27",
    "morgan": "^1.9.1",
    "mysql": "^2.17.1",
    "mysql2": "^2.0.2",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^5.21.2",
    "sequelize-typescript": "^1.1.0",
    "typedi": "^0.8.0",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/express": "^4.17.2",
    "@types/jest": "^24.0.23",
    "@types/node": "^12.12.14",
    "@types/passport": "^1.0.2",
    "@typescript-eslint/eslint-plugin": "^2.13.0",
    "@typescript-eslint/parser": "^2.13.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.8.0",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-json": "^2.0.1",
    "eslint-plugin-prettier": "^3.1.2",
    "jest": "^24.9.0",
    "nodemon": "^2.0.1",
    "prettier": "^1.19.1",
    "ts-jest": "^24.2.0",
    "ts-node": "^8.5.4",
    "tslint": "^5.20.1",
    "typescript": "^3.7.3"
  },
  "bugs": {
    "url": "https://github.com/mass-immersion-approach"
  },
  "homepage": "https://github.com/mass-immersion-approach"
}
