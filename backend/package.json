{
  "name": "mia-experiment-server",
  "version": "1.0.0",
  "description": "MIA Experiment Server",
  "main": "src/app.ts",
  "scripts": {
    "start": "nodemon",
    "build": "tsc",
    "inspect": "nodemon --inspect src/app.ts",
    "migrate": "npx sequelize-cli db:migrate",
    "seed": "npx sequelize-cli db:seed:all",
    "force-sync-dev": "cross-env NODE_ENV=development ts-node src/migrate.ts",
    "force-sync-prod": "cross-env NODE_ENV=production ts-node src/migrate.ts",
    "test": "jest",
    "lint": "npm run lint:js ",
    "lint:eslint": "eslint --ignore-path .gitignore --ext .ts",
    "lint:js": "npm run lint:eslint src/",
    "lint:fix": "npm run lint:js -- --fix"
  },
  "author": "Shan Rauf",
  "license": "ISC",
  "dependencies": {
    "agenda": "^2.2.0",
    "agendash": "^1.0.0",
    "bcryptjs": "^2.4.3",
    "celebrate": "^11.0.0",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "cross-env": "^6.0.3",
    "dotenv": "^8.2.0",
    "errorhandler": "^1.5.1",
    "event-dispatch": "^0.4.1",
    "eventemitter3": "^4.0.0",
    "express": "^4.17.1",
    "express-basic-auth": "^1.2.0",
    "express-jwt": "^5.3.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "mailgun-js": "^0.22.0",
    "method-override": "^3.0.0",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.27",
    "mongoose": "^5.7.14",
    "morgan": "^1.9.1",
    "mysql2": "^2.0.2",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^5.21.2",
    "sequelize-typescript": "^1.1.0",
    "typedi": "^0.8.0",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@types/agenda": "^2.0.6",
    "@types/bcrypt": "^3.0.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/express": "^4.17.2",
    "@types/jest": "^24.0.23",
    "@types/lodash": "^4.14.149",
    "@types/mongoose": "^5.5.32",
    "@types/node": "^12.12.14",
    "@types/passport": "^1.0.2",
    "@typescript-eslint/eslint-plugin": "^2.10.0",
    "@typescript-eslint/parser": "^2.10.0",
    "eslint": "^6.7.2",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-prettier": "^3.1.1",
    "jest": "^24.9.0",
    "nodemon": "^2.0.1",
    "prettier": "^1.19.1",
    "ts-jest": "^24.2.0",
    "ts-node": "^8.5.4",
    "tslint": "^5.20.1",
    "typescript": "^3.7.3"
  },
  "bugs": {
    "url": "https://github.com/mass-immersion-approach"
  },
  "homepage": "https://github.com/mass-immersion-approach"
