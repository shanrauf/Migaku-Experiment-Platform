{
  "name": "mia-experiment-server",
  "version": "1.0.0",
  "description": "MIA Experiment Server",
  "main": "src/server.ts",
  "scripts": {
    "start": "cross-env NODE_ENV=development nodemon",
    "setup-dev": "npm run force-sync-dev && npm run seed",
    "debug-prod": "cross-env NODE_ENV=production node ./build/server.js",
    "start-prod": "sudo pm2 start ./build/server.js",
    "test": "jest --detectOpenHandles --forceExit --no-cache",
    "build": "cross-env NODE_ENV=production webpack --debug --progress --display-error-details --verbose --config webpack.config.js",
    "inspect": "nodemon --inspect src/server.ts",
    "seed": "npx sequelize-cli db:seed:all",
    "seed:undo": "npx sequelize-cli db:seed:undo:all",
    "force-sync-dev": "cross-env NODE_ENV=development ts-node scripts/db/migrate.ts",
    "lint": "eslint --ignore-path .eslintignore --ext .js,.ts ./src ./tests",
    "lint:fix": "npm run lint -- --fix"
  },
  "author": "Shan Rauf",
  "license": "ISC",
  "dependencies": {
    "@sendgrid/mail": "^6.5.1",
    "axios": "^0.19.1",
    "body-parser": "^1.19.0",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.11.0",
    "cookie-parser": "^1.4.4",
    "cookie-session": "^1.4.0",
    "cross-env": "^6.0.3",
    "discord.js": "^11.5.1",
    "dotenv": "^8.2.0",
    "errorhandler": "^1.5.1",
    "event-dispatch": "^0.4.1",
    "eventemitter3": "^4.0.0",
    "express": "^4.17.1",
    "express-basic-auth": "^1.2.0",
    "helmet": "^3.22.0",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.27",
    "morgan": "^1.9.1",
    "mysql2": "^2.1.0",
    "passport": "^0.4.1",
    "passport-oauth2": "^1.5.0",
    "prettier": "^2.0.2",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^5.21.3",
    "sequelize-typescript": "^1.1.0",
    "typedi": "^0.8.0",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.8.3",
    "@types/bluebird": "^3.5.29",
    "@types/express": "^4.17.2",
    "@types/jest": "^24.9.1",
    "@types/node": "^13.1.8",
    "@types/passport": "^1.0.2",
    "@types/passport-oauth2": "^1.4.8",
    "@types/supertest": "^2.0.8",
    "@types/validator": "^12.0.1",
    "@typescript-eslint/eslint-plugin": "^2.25.0",
    "@typescript-eslint/parser": "^2.25.0",
    "clean-webpack-plugin": "^3.0.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-jest": "^23.8.2",
    "eslint-plugin-prettier": "^3.1.2",
    "faker": "^4.1.0",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "jest": "^24.9.0",
    "moxios": "^0.4.0",
    "nodemon": "^2.0.2",
    "supertest": "^4.0.2",
    "terser-webpack-plugin": "^2.3.3",
    "ts-jest": "^24.3.0",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.6.2",
    "typescript": "^3.8.3",
    "webpack": "^4.41.5",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "^3.3.10",
    "webpack-node-externals": "^1.7.2"
  },
  "bugs": {
    "url": "https://github.com/mass-immersion-approach"
  },
  "homepage": "https://github.com/mass-immersion-approach"
}
