{
  "name": "mia-experiment-server",
  "version": "1.0.0",
  "description": "MIA Experiment Server",
  "main": "src/app.ts",
  "scripts": {
    "start": "cross-env NODE_ENV=development nodemon",
    "setup-dev": "npm run force-sync-dev && npm run seed",
    "start-prod": "cross-env NODE_ENV=production PORT=4000 node build/app.js",
    "test": "jest --detectOpenHandles --forceExit",
    "build": "cross-env NODE_ENV=production webpack --config webpack.config.js",
    "build-tsc": "tsc",
    "force-sync-then-start-prod": "cross-env NODE_ENV=production FORCE_SYNC=true node build/app.js",
    "inspect": "nodemon --inspect src/app.ts",
    "seed": "npx sequelize-cli db:seed:all",
    "seed:undo": "npx sequelize-cli db:seed:undo:all",
    "seed-test": "npx sequelize-cli db:seed:all --env test",
    "seed-prod": "npx sequelize-cli db:seed:all --env production",
    "force-sync-dev": "cross-env NODE_ENV=development ts-node src/migrate.ts",
    "force-sync-prod": "cross-env NODE_ENV=production ts-node src/migrate.ts --env production",
    "lint": "npm run lint:eslint ",
    "lint:eslint": "eslint --ignore-path .gitignore --ext .ts src/",
    "lint:fix": "npm run lint:eslint -- --fix"
  },
  "author": "Shan Rauf",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.19.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "celebrate": "^11.0.0",
    "cookie-parser": "^1.4.4",
    "cross-env": "^6.0.3",
    "dotenv": "^8.2.0",
    "errorhandler": "^1.5.1",
    "event-dispatch": "^0.4.1",
    "eventemitter3": "^4.0.0",
    "express": "^4.17.1",
    "express-basic-auth": "^1.2.0",
    "express-jwt": "^5.3.1",
    "jsonwebtoken": "^8.5.1",
    "mailgun-js": "^0.22.0",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.27",
    "morgan": "^1.9.1",
    "mysql": "^2.17.1",
    "mysql2": "^2.0.2",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^5.21.3",
    "sequelize-typescript": "^1.1.0",
    "typedi": "^0.8.0",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.7.7",
    "@types/bcryptjs": "^2.4.2",
    "@types/bluebird": "^3.5.29",
    "@types/express": "^4.17.2",
    "@types/jest": "^24.0.25",
    "@types/joi": "^14.3.4",
    "@types/node": "^12.12.22",
    "@types/passport": "^1.0.2",
    "@types/supertest": "^2.0.8",
    "@types/validator": "^12.0.1",
    "@typescript-eslint/eslint-plugin": "^2.13.0",
    "@typescript-eslint/parser": "^2.13.0",
    "babel-jest": "^24.9.0",
    "clean-webpack-plugin": "^3.0.0",
    "eslint": "^6.1.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.8.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^1.7.0",
    "faker": "^4.1.0",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "jest": "^24.9.0",
    "moxios": "^0.4.0",
    "nodemon": "^2.0.1",
    "prettier": "^1.19.1",
    "supertest": "^4.0.2",
    "ts-jest": "^24.2.0",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.5.4",
    "tslint": "^5.20.1",
    "typescript": "^3.7.3",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^4.41.5",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "^3.3.10",
    "webpack-node-externals": "^1.7.2"
  },
  "bugs": {
    "url": "https://github.com/mass-immersion-approach"
  },
  "homepage": "https://github.com/mass-immersion-approach"
}
