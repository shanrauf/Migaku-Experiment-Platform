  "scripts": {
    "start": "cross-env NODE_ENV=development DEBUG=true webpack-dev-server --config webpack.config.js --progress",
    "staging": "cross-env NODE_ENV=production DEBUG=true webpack-dev-server --config webpack.config.js --progress",
