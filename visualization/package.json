{
  "name": "vue-spa-boilerplate",
  "version": "1.0.0",
  "description": "Boilerplate for Vue single-page applications.",
  "author": "Shan Rauf",
  "license": "ISC",
  "scripts": {
    "start": "cross-env NODE_ENV=development DEBUG=true nodemon index.js",
    "dev-client": "cross-env NODE_ENV=development DEBUG=true webpack-dev-server --config webpack.config.js --progress",
    "build": "cross-env NODE_ENV=production DEBUG=true webpack --config webpack.config.js --progress",
    "test": "echo 'create test script'",
    "analyze": "cross-env NODE_ENV=development DEBUG=true BUNDLE_ANALYZER=true nodemon --watch server index.js",
    "deploy": "npm run prod && firebase deploy",
    "lint": "eslint --ext .js,.vue src",
    "lint:fix": "eslint --fix --ext .js,.vue src",
    "prettier": "prettier \"src/**/*.{js,vue}\"",
    "prettier:write": "npm run prettier -- --write",
    "loc": "git ls-files | xargs wc -l"
  },
  "dependencies": {
    "@babel/runtime": "^7.7.2",
    "apexcharts": "^3.10.1",
    "archiver": "^3.1.1",
    "body-parser": "^1.15.2",
    "connect-history-api-fallback": "^1.6.0",
    "core-js": "^3.2.1",
    "lodash": "^4.17.15",
    "material-icons": "^0.3.1",
    "vue": "^2.6.10",
    "vue-apexcharts": "^1.5.1",
    "vue-router": "^3.1.2",
    "vuetify": "^2.0.5",
    "vuetify-loader": "^1.3.0",
    "vuex": "^3.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.7.2",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/preset-env": "^7.7.1",
    "@fortawesome/fontawesome-free": "^5.10.1",
    "@mdi/font": "^3.9.97",
    "babel-eslint": "^10.0.2",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "compression-webpack-plugin": "^2.0.0",
    "copy-webpack-plugin": "^5.0.4",
    "core-js": "^3.1.4",
    "cross-env": "^5.2.1",
    "css-loader": "^3.2.0",
    "deepmerge": "^4.0.0",
    "eslint": "^6.1.0",
    "eslint-plugin-vue": "^5.2.3",
    "eslint-plugin-vuetify": "^1.0.0-beta.3",
    "file-loader": "^4.2.0",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "html-webpack-harddisk-plugin": "^1.0.1",
    "html-webpack-plugin": "^4.0.0-beta.8",
    "image-webpack-loader": "^5.0.0",
    "material-design-icons-iconfont": "^5.0.1",
    "mini-css-extract-plugin": "^0.5.0",
    "nodemon": "^1.19.4",
    "optimize-css-assets-webpack-plugin": "~3.2",
    "postcss-loader": "^3.0.0",
    "sass": "^1.22.10",
    "sass-loader": "^7.3.1",
    "uglifyjs-webpack-plugin": "^1.3.0",
    "url-loader": "^2.1.0",
    "vue-loader": "^15.7.1",
    "vue-style-loader": "~4.1",
    "vue-template-compiler": "^2.6.10",
    "webpack": "^4.41.2",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "~3.2",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-dev-server": "~3.1",
    "webpack-hot-middleware": "^2.24.4",
    "webpack-merge": "^4.2.1"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
